{
	"name": "stodio",
	"compatibility_date": "2025-10-01",
	"main": "./server.js",
	"build": {
		"command": "npx deno install --allow-scripts && npx deno task build"
	},
	"assets": {
		"directory": "./_fresh/client"
	},
	"vars": {
		"SKIP_DEPENDENCY_INSTALL": "1"
	},
	"observability": {
		"logs": {
			"enabled": true
		},
		"enabled": true
	}
}
