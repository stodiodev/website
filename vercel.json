{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "trailingSlash": false,
  "devCommand": "deno task start",
  "functions": {
    "api/handler.js": {
      "runtime": "vercel-deno@2.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/handler"
    }
  ]
}